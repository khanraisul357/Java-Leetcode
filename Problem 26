//argument passed is of Integer array type-->removeDuplicates(array)
class Solution {
    public int removeDuplicates(int[] arr) {
        int i=0;
		int j=i+1,k=0;
		System.out.println(k);
		
		while(j<arr.length) {
			if(arr[i]==arr[j]) {
				j++;
			}
			else {
				i++;
				k++;
				arr[i]=arr[j];
			}
		}

		
		return k+1;
		
    }
}
